  "dependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.19",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "coveralls": "^3.1.1",
    "polished": "^4.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "microbundle build --tsconfig tsconfig.json --jsx React.createElement  --compress=false",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook --no-dll --quiet",
    "lint": "eslint . --ext tsx",
    "lint:fix": "eslint . --ext tsx --fix",
    "prepare": "husky install",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "pre-commit": "yarn test && npx lint-staged",
    "coverall": "npm run coverage  && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "plop": "plop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,mdx}": [
      "prettier --write"
    ]
  },
  "np": {
    "yarn": false,
    "contents": "dist"
  },
  "browserslist": {
    "production": [
      "> 0.5%",
      "last 2 versions",
      "Firefox ESR",
      "not dead",
      "IE 11",
      "not IE 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.4.13",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/addon-storysource": "^6.4.13",
    "@storybook/addon-viewport": "^6.4.13",
    "@storybook/addons": "^6.4.13",
    "@storybook/builder-webpack5": "^6.4.13",
    "@storybook/manager-webpack5": "^6.4.13",
    "@storybook/node-logger": "^6.4.13",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.13",
    "@storybook/theming": "^6.4.13",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/styled-components": "^5.1.20",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.7.0",
    "eslint-plugin-mdx": "^1.16.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.7",
    "microbundle": "^0.14.2",
    "np": "^7.6.0",
    "plop": "^3.0.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0"
  },
